library(tidyverse)
library(readxl)
heart = read_excel("heart.xlsx")
#View(heart)
ggplot(data = heart, aes(x = fbs, fill = target)) +
geom_bar(color = "black", position = "fill", width = 0.4) +
labs(x = "if fasting blood sugar level is above 120 mg/L",
y = "Proportion of heart disease",
