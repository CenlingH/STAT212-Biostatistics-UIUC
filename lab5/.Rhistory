heart %>%
group_by(fbs,target) %>%
summarise(count=n())
prop.test(x=c(116,22),
n=c(258,45),
alternative="less")
prop.test(x=c(116,22),
n=c(258,45),
alternative="less")
ggplot(data = heart, aes(x = exang, fill = target)) +
geom_bar(color = "black", position = "fill", width = 0.4) +
labs(x = "if people have experienced an Exercise-Induced Angina",
y = "Proportion of heart disease",
fill = "Exercise-Induced Angina experience",
title = "Relationship between exang and heart disease")+
theme_bw() +
scale_y_continuous(breaks = seq(0,1,0.1))
heart %>%
group_by(fbs,target) %>%
summarise(count=n())
prop.test(x=c(116,22),
n=c(258,45),
alternative="less")
heart %>%
group_by(exang,target) %>%
summarise(count=n())
prop.test(x=c(62,76),
n=c(204,99),
alternative="less")
ggplot(data = heart, aes(x = exang, y = chol, color = exang)) +
geom_jitter(width = 0.05) +
stat_summary(fun = mean, fun.min = mean, fun.max = mean, geom = "errorbar", color = "black", width = 0.25, size = 1.5) +
labs(x = "if people have experienced exercise included angias", y = "cholesterol level", title = "relation between exang and chol")
ggplot(data = heart, aes(x = exang, y = chol, color = exang)) +
geom_jitter(width = 0.05) +
stat_summary(fun = mean, fun.min = mean, fun.max = mean, geom = "errorbar", color = "black", width = 0.1, size = 1.5) +
labs(x = "if people have experienced exercise included angias", y = "cholesterol level", title = "relation between exang and chol")
ggplot(data = heart, aes(x = exang, y = chol, color = exang)) +
geom_jitter(width = 0.05) +
stat_summary(fun = mean, fun.min = mean, fun.max = mean, geom = "errorbar", color = "black", width = 0.15, size = 1.5) +
labs(x = "if people have experienced exercise included angias", y = "cholesterol level", title = "relation between exang and chol")
t.test(data = heart,
chol ~ exang,
var.equal = FALSE)
